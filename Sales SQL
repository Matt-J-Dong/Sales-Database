CREATE TABLE inventory(
id INTEGER PRIMARY KEY,
item TEXT UNIQUE NOT NULL,
stock INTEGER,
price FLOAT,
in_stock BOOLEAN DEFAULT true
);
DROP TABLE IF EXISTS inventory;
ALTER TABLE inventory ADD in_stock BOOLEAN DEFAULT true;

